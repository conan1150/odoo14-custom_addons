<odoo>
  <data>
    <record id="open_merge_delivery_order_wizard" model="ir.ui.view">
      <field name="name">open.merge.delivery.order.wizard</field>
      <field name="model">stock.picking</field>
      <field name="inherit_id" ref="stock.vpicktree" />
      <field name="arch" type="xml">
        <xpath expr="//button[@name='do_unreserve']" position="before">
          <button name="open_wiz_merge" string="Requisition" type="object" />
        </xpath>
      </field>
    </record>
  </data>
</odoo>